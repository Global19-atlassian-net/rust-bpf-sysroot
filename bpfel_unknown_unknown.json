{
  "llvm-target": "bpf"
, "data-layout": "e-m:e-p:64:64-i64:64-n32:64-S128"
, "target-endian": "little"
, "target-pointer-width": "64"
, "target-c-int-width": "64"
, "target-os": "linux"
, "target-env": "gnu"
, "target_vendor": "unknown"
, "os": "linux"
, "arch": "bpf"
, "linker-flavor": "ld.lld"
, "dynamic-linking": true
, "no-builtins": true
, "panic-strategy": "abort"
, "relro-level": "off" 
, "linker": "./llvm-native/bin/ld.lld"
, "pre-link-args": {
    "ld.lld": [ "-z", "notext", "--Bdynamic", "-shared", "--entry", "entrypoint", "bpf.ld" ]
  }
}

